{

  "tags": {
    "allowUnknownTags": true
  },

  "opts": {
    "destination": "./docs",
    "template": "./node_modules/foodoc/template"
  },

  "sourceType": "module",

  "plugins": [
    "plugins/markdown"
  ],

  "templates": {

    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },

    "dateFormat": "MMMM Do YYYY, h:mm:ss a",

    "systemName": "djipcam",
    "collapseSymbols": true

  }

}